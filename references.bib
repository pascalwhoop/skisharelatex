@article{Weber2011133,
title = "Accountability in the Internet of Things ",
journal = "Computer Law & Security Review ",
volume = "27",
number = "2",
pages = "133 - 138",
year = "2011",
note = "",
issn = "0267-3649",
doi = "http://dx.doi.org/10.1016/j.clsr.2011.01.005",
url = "http://www.sciencedirect.com/science/article/pii/S0267364911000069",
author = "Rolf H. Weber",
keywords = "Accountability",
keywords = "Legitimacy",
keywords = "Sanctions",
keywords = "Standards",
keywords = "Surveillance ",
abstract = "Accountability of governing bodies in the Internet of Things (IoT) is of major importance and requires a partly different approach than applied in the (general) Internet. Improving accountability makes the implementation of new general principles necessary in order to provide for a stable and foreseeable legal framework on which businesses can rely. In particular, standards need to be introduced that hold governing bodies accountable, information should be made more readily available and beneficiaries of accountability must be able to impose some sort of sanction on the accountable in case of non-compliance. Improving accountability by creating such framework also supports the betterment of security in the Internet of Things. "
}



